{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react-native",
    "paths": {
      "@api": ["src/api"],
      "@assets": ["./assets"],
      "@constants": ["src/constants"],
      "@contexts": ["src/contexts"],
      "@components": ["src/components"],
      "@screens": ["src/screens"],
      "@config": ["src/config"],
      "@utils": ["src/utils"],
      "@theme": ["src/theme"],
      "@types": ["src/types"],
      "@services": ["src/services"],
      "@navigation": ["src/navigation"],
      "@navigation/*": ["src/navigation/*"],
      "@hooks": ["./src/hooks"],
      "@redux/*": ["src/redux/*"],
      "@i18n": ["src/i18n"],
      "@helpers": ["src/helpers"]
    }
  }
}
